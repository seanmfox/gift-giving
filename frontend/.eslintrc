{
	"extends": ["eslint:recommended", "plugin:react/recommended"],
	"parser": "babel-eslint",
	"parserOptions": {
		"ecmaVersion": 2018,
		"sourceType": "module",
		"ecmaFeatures": {
			"jsx": true,
			"modules": true,
			"classes": true
		}
	},
	"env": {
		"browser": true,
		"node": true
	},
	"rules": {
		"no-console": 0,
		"prefer-const": [
			"error",
			{
				"destructuring": "all"
			}
		],
		"strict": 0,
		"import/prefer-default export": 0,
		"import": 0,
		"no-mixed-spaces-and-tabs": 0,
		"import/extensions": 0,
		"react/react-in-jsx-scope": 0,
		"react/jsx-uses-react": 1,
		"react/jsx-filename-extension": [
			1,
			{
				"extensions": [".js", ".jsx"]
			}
		],
		"react/prop-types": 0
	}
}
